# PostgreSQL Environment Configuration Example
# Copy this to .env and update values as needed

# Database Configuration (PostgreSQL)
DATABASE_URL=postgres://freeradical:password@localhost:5432/freeradical

# Application Configuration
APP_BIND_ADDRESS=0.0.0.0
APP_BIND_PORT=8000
APP_BASE_URL=http://localhost:8001
APP_JWT_KEY=change-this-in-production-to-a-secure-secret
APP_MAX_REQ=100

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Logging
RUST_LOG=info

# Docker PostgreSQL Configuration
POSTGRES_USER=freeradical
POSTGRES_PASSWORD=password
POSTGRES_DB=freeradical
POSTGRES_PORT=5432
